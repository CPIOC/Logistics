<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cpic.taylor.logistics">

    <uses-sdk android:minSdkVersion="14"
        android:targetSdkVersion="23"/>


    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <uses-permission android:name="android.permission.GET_TASKS" />
    <uses-permission android:name="android.permission.INTERACT_ACROSS_USERS_FULL" />

    <!-- 查看 Wi-Fi 状态 -->
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <!-- 查看网络状态 -->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <!-- 录音 -->
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <!-- 控制振动器 -->
    <uses-permission android:name="android.permission.VIBRATE" />
    <!-- 防止设备休眠 -->
    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <uses-permission android:name="android.permission.WRITE_SETTINGS" />
    <uses-permission android:name="android.permission.MODIFY_AUDIO_SETTINGS" />
    <uses-permission android:name="android.permission.KILL_BACKGROUND_PROCESSES" />
    <!--获取联系人信息，demo中演示发送通讯录消息-->
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
    <application
        android:name=".base.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <meta-data
            android:name="com.amap.api.v2.apikey"
            android:value="47efd4c8532f9cf4b68057b9427bec15" />

        <activity android:name=".activity.ChatMainActivity">
        </activity>
        <activity android:name=".activity.HomeActivity"
            android:windowSoftInputMode="adjustPan">

        </activity>
        <activity android:name=".activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.ForgetPwdActivity"></activity>
        <activity android:name=".activity.ChangePwdActivity"></activity>
        <activity android:name=".activity.RegisterActivity"></activity>
        <activity android:name=".activity.ChooseAreaActivity"></activity>

        <service android:name="com.amap.api.location.APSService" >
        </service>
        <!--融云声明文件-->
        <!--登录页面-->
        <activity
            android:name=".RongCloudActivity.RongCloudLoginActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateHidden">
        </activity>
        <!--主页面-->
        <activity
            android:name=".RongCloudActivity.MainActivity"
            android:launchMode="singleTask"
            android:screenOrientation="portrait">

            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />

                <data
                    android:host="com.cpic.taylor.logistics"
                    android:pathPrefix="/push_message"
                    android:scheme="rong" />
            </intent-filter>
        </activity>
        <!--注册页面-->
        <activity
            android:name=".RongCloudActivity.RegisterActivity"
            android:screenOrientation="portrait"></activity>
        <!--发起聊天页面-->
        <activity
            android:name=".RongCloudActivity.FriendListActivity"
            android:screenOrientation="portrait"></activity>
        <!--通讯录-->
        <activity
            android:name=".RongCloudActivity.ContactsActivity"
            android:screenOrientation="portrait"></activity>

        <!--我的账号-->
        <activity
            android:name=".RongCloudActivity.MyAccountActivity"
            android:screenOrientation="portrait"></activity>
        <!--修改昵称-->
        <activity
            android:name=".RongCloudActivity.UpdateNameActivity"
            android:screenOrientation="portrait"></activity>
        <!--新消息提醒-->
        <activity
            android:name=".RongCloudActivity.NewMessageRemindActivity"
            android:screenOrientation="portrait"></activity>
        <!--消息免打扰-->
        <activity
            android:name=".RongCloudActivity.DisturbActivity"
            android:screenOrientation="portrait"></activity>
        <!--隐私-->
        <activity
            android:name=".RongCloudActivity.PrivacyActivity"
            android:screenOrientation="portrait"></activity>
        <!--关于融云-->
        <activity
            android:name=".RongCloudActivity.AboutRongCloudActivity"
            android:screenOrientation="portrait"></activity>
        <!--关于融云－更新日志-->
        <activity android:name=".RongCloudActivity.UpdateLogActivity"></activity>
        <!--关于融云－功能介绍-->
        <activity android:name=".RongCloudActivity.FunctionIntroducedActivity"></activity>
        <!--关于融云－开发者文档-->
        <activity android:name=".RongCloudActivity.DocumentActivity"></activity>
        <!--关于融云－官方网站-->
        <activity android:name=".RongCloudActivity.RongWebActivity"></activity>
        <!--黑名单列表-->
        <activity
            android:name=".RongCloudActivity.BlackListActivity"
            android:screenOrientation="portrait"></activity>

        <!--群组详情-->
        <activity
            android:name=".RongCloudActivity.GroupDetailActivity"
            android:screenOrientation="portrait"></activity>
        <!--SOSO 地图位置-->

        <!--点击会话页面图片展示页面-->
        <activity
            android:name=".RongCloudActivity.PhotoActivity"
            android:screenOrientation="portrait" />
        <!--搜索好友-->
        <activity
            android:name=".RongCloudActivity.SearchFriendActivity"
            android:screenOrientation="portrait"></activity>
        <!--新的朋友列表-->
        <activity
            android:name=".RongCloudActivity.NewFriendListActivity"
            android:screenOrientation="portrait"></activity>
        <!--更新讨论组名称-->
        <activity
            android:name=".RongCloudActivity.UpdateDiscussionActivity"
            android:screenOrientation="portrait"></activity>
        <!--修改群名片-->
        <activity
            android:name=".RongCloudActivity.UpdateGroupUserInfoActivity"
            android:screenOrientation="portrait"></activity>

        <!--个人详情-->
        <activity
            android:name=".RongCloudActivity.PersonalDetailActivity"
            android:screenOrientation="portrait"></activity>


        <activity
            android:name=".RongCloudActivity.PublicServiceActivity"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"></activity>

        <activity
            android:name=".RongCloudActivity.PublicServiceSearchActivity"
            android:screenOrientation="portrait"></activity>


        <activity android:name=".RongCloudActivity.NewTextMessageActivity"></activity>

        <!--公众-->
        <activity
            android:name=".RongCloudActivity.PublicServiceInfoActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
                <data
                    android:host="com.cpic.taylor.logistics"
                    android:pathPrefix="/publicServiceProfile"
                    android:scheme="rong" />
            </intent-filter>

            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
                <data
                    android:host="com.cpic.taylor.logistics"
                    android:pathPrefix="/publicServiceInfo"
                    android:scheme="rong" />
            </intent-filter>
        </activity>

        <!--设置页面-->
        <activity
            android:name=".RongCloudActivity.ConversationSettingActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />

                <data
                    android:host="com.cpic.taylor.logistics"
                    android:pathPrefix="/conversationSetting/"
                    android:scheme="demo" />
            </intent-filter>

        </activity>

        <!--会话页面-->
        <activity
            android:name=".RongCloudActivity.ConversationActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateHidden|adjustResize">

            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />

                <data
                    android:host="com.cpic.taylor.logistics"
                    android:pathPrefix="/conversation/"
                    android:scheme="rong" />
            </intent-filter>

        </activity>
        <!--聚合会话列表-->
        <activity
            android:name=".RongCloudActivity.SubConversationListActivity"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateHidden|adjustResize">

            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
                <data
                    android:host="com.cpic.taylor.logistics"
                    android:path="/subconversationlist"
                    android:scheme="rong" />
            </intent-filter>
        </activity>

        <!--会话列表-->
        <activity
            android:name=".RongCloudActivity.ConversationListActivity"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateHidden|adjustResize">

            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
                <data
                    android:host="com.cpic.taylor.logistics"
                    android:path="/conversationlist"
                    android:scheme="rong" />
            </intent-filter>
        </activity>


        <!--begin rongcloud-->
        <activity
            android:name="io.rong.voipkit.activity.CallSideActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />

                <data
                    android:host="com.cpic.taylor.logistics"
                    android:pathPrefix="/VoIPCall"
                    android:scheme="rong" />
            </intent-filter>
        </activity>

        <activity
            android:name="io.rong.voipkit.activity.CalledSideActivity"
            android:excludeFromRecents="true"
            android:screenOrientation="portrait">

            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />

                <data
                    android:host="com.cpic.taylor.logistics"
                    android:pathPrefix="/VoIPAccept"
                    android:scheme="rong" />
            </intent-filter>
        </activity>

        <service
            android:name="io.rong.imlib.ipc.RongService"
            android:process=":ipc"></service>
        <service android:name="io.rong.imlib.ReConnectService" />

        <receiver android:name="io.rong.imlib.ConnectChangeReceiver" />
        <receiver android:name="io.rong.imlib.ipc.PushMessageReceiver">
            <intent-filter>
                <action android:name="io.rong.push.message" />
            </intent-filter>
        </receiver>
        <receiver
            android:name="io.rong.imlib.HeartbeatReceiver"
            android:process=":ipc" />

        <activity
            android:name="io.rong.imkit.tools.SelectPictureActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="io.rong.imkit.tools.PreviewPictureActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="io.rong.imkit.tools.RongWebviewActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="io.rong.imkit.widget.provider.TakingPicturesActivity"
            android:configChanges="orientation|keyboardHidden"
            android:screenOrientation="portrait" />

        <service
            android:name="io.rong.push.PushService"
            android:process="io.rong.push"> <!-- Waring: The name of the push process can't be changed!!! -->
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT" />

                <action android:name="io.rong.push" />
            </intent-filter>
        </service>
        <service
            android:name="io.rong.push.CommandService"
            android:process="io.rong.push"> <!-- Waring: The name of the push process can't be changed!!! -->
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT" />

                <action android:name="io.rong.command" />
            </intent-filter>
        </service>

        <receiver
            android:name="io.rong.push.PushReceiver"
            android:process="io.rong.push"> <!-- Waring: The name of the push process can't be changed!!! -->
            <intent-filter>
                <action android:name="io.rong.push.HeartBeat" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.net.conn.CONNECTIVITY_CHANGE" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.USER_PRESENT" />
                <action android:name="android.intent.action.ACTION_POWER_CONNECTED" />
                <action android:name="android.intent.action.ACTION_POWER_DISCONNECTED" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.PACKAGE_REMOVED" />

                <data android:scheme="package" />
            </intent-filter>
        </receiver>

        <meta-data
            android:name="RONG_CLOUD_APP_KEY"
            android:value="z3v5yqkbv8v30" />

        <!--end rongcloud-->

        <!-- 请将CHANNAL改为app的发布渠道 < -->
        <meta-data
            android:name="InstallChannel"
            android:value="CHANNAL" />


        <!-- soso -->
        <meta-data
            android:name="TencentMapSDK"
            android:value="7JYBZ-4Y3W4-JMUU7-DJHQU-NOYH7-SRBBU" />
    </application>

</manifest>